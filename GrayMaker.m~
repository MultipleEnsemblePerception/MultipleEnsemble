fileList = dir('UCB_Stimuli/*.png');
nfiles = length(fileList);

images = cell(1, nfiles);

for i=1:nfiles
    currentfilename = fileList(i).name;
    currentimage = imread(sprintf('UCB_Stimuli/%s',currentfilename));
    images{i} = rgb2gray(currentimage);
end

for i=1:length(images)
    for 1:
end
